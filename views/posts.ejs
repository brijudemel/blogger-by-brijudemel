<%- include('partials/header'); %>
	<h1>Home</h1>
	<p><%=startingContent%></p>
	<br>
	<% var i=0 %>
	<% posts.forEach(function(post){%>
		<h1><%= post.title%></h1>
		<p><%= post.content.substring(0,100)+"..."%><a href="/<%=user%>/posts/<%= i %>">Read More</a></p>
		<% i++ %>
	<% }); %>
<%- include('partials/footer'); %>